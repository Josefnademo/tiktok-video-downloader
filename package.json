{
  "name": "media-downloader-desktop",
  "version": "1.0.0",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "start": "electron .",
    "build:win": "electron-builder --win",
    "build:linux": "electron-builder --linux AppImage",
    "build:mac": "electron-builder --mac"
  },
  "dependencies": {
    "body-parser": "^2.2.2",
    "bottleneck": "^2.19.5",
    "cors": "^2.8.6",
    "dotenv": "^16.6.1",
    "express": "^5.2.1",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.2",
    "m3u8-parser": "^7.1.0",
    "node-fetch": "^3.3.2"
  },
  "description": "TikTok Downloader",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Josefnademo/tiktok-video-downloader.git"
  },
  "keywords": [],
  "author": "Recorayt",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Josefnademo/tiktok-video-downloader/issues"
  },
  "homepage": "https://github.com/Josefnademo/tiktok-video-downloader#readme",
  "devDependencies": {
    "electron": "^40.0.0",
    "electron-builder": "^26.4.0"
  },
  "build": {
    "appId": "com.tiktok-downloader.app",
    "productName": "TikTok Downloader",
    "compression": "maximum",
    "electronLanguages": [
      "en-US"
    ],
    "directories": {
      "output": "dist"
    },
    "files": [
      "main.js",
      "server.js",
      "public/**/*",
      "src/**/*",
      "package.json",
      "node_modules/**/*",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!**/node_modules/*.d.ts",
      "!**/node_modules/.bin"
    ],
    "win": {
      "target": "portable",
      "icon": "public/assets/icon.png"
    },
    "asar": true,
    "asarUnpack": [
      "node_modules/ffmpeg-static/**",
      "node_modules/ffprobe-static/**"
    ]
  }
}
